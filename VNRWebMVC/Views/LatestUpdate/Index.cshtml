@model VNRWebMVC.ViewModels.LatestUpdateVM

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-group">
        @Html.LabelFor(model => model.Information)
        @Html.TextBoxFor(model => model.Information, new { @class = "form-control" })
        @Html.ValidationMessageFor(model => model.Information)
    </div>
    <div class="form-group">
        <div class="col-md-10">
            @Html.EditorFor(model => model.CreatedBy, new { htmlAttributes = new { style = "display: none;", Value = 40 } })
        </div>
    </div>
    <button type="submit" class="btn btn-primary">Create</button>
}



